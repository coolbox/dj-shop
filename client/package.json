{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.9.0",
    "react-scripts": "3.2.0",
    "format-duration": "^1.3.1",
    "node-sass": "^4.13.0",
    "pluralize": "^8.0.0",
    "react-router-dom": "^5.0.1",
    "sass": "^1.23.0"
  },
  "scripts": {
    "sass:build": "sass --update src/styles/scss:src/styles/css",
    "sass:watch" : "sass --watch src/styles/scss:src/styles/css",
